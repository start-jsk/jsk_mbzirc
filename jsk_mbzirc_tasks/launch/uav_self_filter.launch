<launch>
  <arg name="image" default="/camera/rgb/image_rect_color" />
  <node pkg="jsk_mbzirc_tasks" type="uav_self_filter_node" 
	name="uav_self_filter" output="screen" >
    <remap from="input" to="$(arg image)" />
    <remap from="target" to="/uav_self_filter/output/image" />
    
    <!-- 150 set based on gazebo start podium -->
    <rosparam>
      ~mask_thresh: 150
    </rosparam>

  </node>
</launch>
