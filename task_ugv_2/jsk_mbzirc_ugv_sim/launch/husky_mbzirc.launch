<launch>

  <arg name="ROBOT_INITIAL_POSE" default="-y 10"/>

<!--Map and Model-->
  <arg name="laser_enabled" default="true"/>
  <arg name="ur5_enabled" default="true"/>
  <arg name="kinect_enabled" default="false"/>
  <arg name="gripper_enabled" default="true"/>
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find husky_gazebo)/launch/husky_empty_world.launch">
    <arg name="world_name" value="$(find jsk_mbzirc_common)/gazebo_model/world/mbzirc_arena_old.world"/>
    <arg name="laser_enabled" value="$(arg laser_enabled)"/>
    <arg name="ur5_enabled" value="$(arg ur5_enabled)"/>
    <arg name="kinect_enabled" value="$(arg kinect_enabled)"/>
    <arg name="gripper_enabled" value="$(arg gripper_enabled)"/>
  </include>

<!--Controllers-->
  <arg name="sim" default="true" />
  <arg name="limited" default="true"/>
  <arg name="debug" default="false" />
  <include file="$(find husky_ur5_moveit_config)/launch/husky_ur5_planning_execution.launch">
    <arg name="sim" value="$(arg sim)"/>
    <arg name="limited" value="$(arg limited)"/>
    <arg name="debug" value="$(arg debug)"/>
  </include>
  <include file="$(find interactive_marker_twist_server)/launch/interactive_markers.launch">
  </include>

<!--rviz-->
  <arg name="debug_rviz" default="false" />
  <arg name="config" default="true" />
  <include file="$(find husky_ur5_moveit_config)/launch/moveit_rviz.launch">
    <arg name="config" value="$(arg config)"/>
    <arg name="debug" value="$(arg debug_rviz)"/>
  </include>


</launch>
